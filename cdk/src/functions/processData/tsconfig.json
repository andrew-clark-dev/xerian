{
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "moduleResolution": "Node",
    "lib": [
      "ES2022"
    ],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "dist",
    "declaration": false,
    "sourceMap": true,
    "paths": {
      "@cdk-src/*": [
        "../../src/*"
      ],
      "@data-schema": [
        "../../../../amplify/data/resource"
      ],
    }
  },
  "include": [
    "../../../src/**/*.ts",
    "../../../stacks/data-import-stack.ts"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}